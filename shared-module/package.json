{
  "name": "shared-module",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": ">=11.11"
  },
  "main": "./dist/index.js",
  "scripts": {
    "start": "NODE_ENV=production node dist/src/index.js",
    "dev": "WATCH_MODE=true nodemon --exec 'ts-node --files --project tsconfig.json' src/index.ts",
    "local": "NODE_ENV=dev WATCH_MODE=true nodemon --exec 'ts-node --files --project tsconfig.json' src/index.ts",
    "build": "if [ \"$NODE_ENV\" = \"dev\" ]; then npm run build:dev ; else npm run build:prod; fi",
    "build:dev": "tsc --project tsconfig.production.json && webpack --config=webpack.config.js --progress",
    "build:prod": "tsc --project tsconfig.production.json",
    "clean": "rm -rf cache dist tmp ts-node",
    "pretest": "yarn lint",
    "test": "ENV_NAME=test jest",
    "ts": "jest --silent --coverage",
    "test:watch": "yarn test --watch",
    "test:types": "tsc --noEmit",
    "test:coverage": "yarn test --coverage",
    "lint": "tslint --project tsconfig.json '{src,test}/**/*.{ts,tsx}'",
    "lint:fix": "tslint --fix --project tsconfig.json '{src,test}/**/*.{ts,tsx}'",
    "format": "prettier --write '{src,test}/**/*.{js,ts,tsx,json,css,md}'"
  },
  "author": "",
  "license": "ISC",
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm test && yarn build:prod"
    }
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "body-parser": "^1.18.3",
    "bootstrap": "^4.1.3",
    "emotion": "^9.2.10",
    "es6-promise": "^4.2.8",
    "lodash": "^4.17.11",
    "ramda": "^0.26.1",
    "superagent": "^4.0.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/jest": "24.0.15",
    "@types/lodash": "^4.14.117",
    "@types/nock": "^9.3.0",
    "@types/node": "12.0.10",
    "@types/ramda": "^0.26.4",
    "@types/superagent": "^3.8.5",
    "@types/supertest": "^2.0.5",
    "@types/webpack-env": "1.13.9",
    "assets-webpack-plugin": "^3.9.5",
    "babel-core": "6.26.3",
    "babel-jest": "24.8.0",
    "clean-webpack-plugin": "0.1.19",
    "copy-webpack-plugin": "^4.5.2",
    "file-loader": "^2.0.0",
    "gulp-uglify": "^3.0.1",
    "hard-source-webpack-plugin": "^0.13.1",
    "husky": "^1.3.1",
    "jest": "^24.5.0",
    "lerna": "^3.15.0",
    "mini-css-extract-plugin": "^0.6.0",
    "nock": "^10.0.1",
    "nodemon": "^1.18.10",
    "prettier": "^1.14.2",
    "react-testing-library": "^6.0.2",
    "redux-mock-store": "^1.5.3",
    "style-loader": "^0.23.1",
    "supertest": "^4.0.2",
    "ts-jest": "24.0.2",
    "ts-loader": "6.0.4",
    "ts-node": "^8.0.3",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-config-standard": "^8.0.1",
    "tslint-react": "^3.6.0",
    "typescript": "^3.3.4000",
    "typescript-json-schema": "^0.36.0",
    "url-loader": "^1.1.1",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-dev-middleware": "^3.6.1",
    "webpack-hot-middleware": "^2.24.3",
    "webpack-manifest-plugin": "^2.0.4"
  }
}
